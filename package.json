{
  "name": "webpack-react-template",
  "version": "0.1.0",
  "private": true,
  "description": "Webpack React Template",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack serve --config config/webpack.dev.js",
    "build": "cross-env NODE_ENV=production webpack --config config/webpack.prod.js",
    "build:live": "serve -s build",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,json,css}",
    "lint": "eslint src/**/*.{js,jsx,ts,tsx}",
    "lint:fix": "eslint src/**/*.{js,jsx,ts,tsx} --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "commit": "git-cz"
  },
  "license": "MIT",
  "author": "kotoyama <t.me/kotoyama>",
  "engines": {
    "node": ">= 12.18.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-transform-modules-commonjs": "^7.12.13",
    "@babel/preset-env": "^7.12.10",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.1",
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.0",
    "@svgr/webpack": "^5.4.0",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^12.1.0",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.2.1",
    "@types/core-js": "^2.5.4",
    "@types/jest": "^27.0.1",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/webpack-env": "^1.16.0",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "babel-jest": "^27.2.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-transform-rename-import": "^2.3.0",
    "case-sensitive-paths-webpack-plugin": "^2.3.0",
    "circular-dependency-plugin": "^5.2.2",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "commitizen": "^4.2.3",
    "cross-env": "^7.0.3",
    "css-loader": "^6.2.0",
    "css-minimizer-webpack-plugin": "^3.0.2",
    "cz-conventional-changelog": "3.3.0",
    "dotenv-webpack": "^7.0.3",
    "effector-logger": "^0.12.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-effector": "^0.2.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.2",
    "eslint-react": "^0.0.4",
    "eslint-webpack-plugin": "^3.0.1",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^6.0.6",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^5.0.0",
    "husky": "^4.2.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.2.0",
    "lint-staged": "^11.1.2",
    "mini-css-extract-plugin": "^2.3.0",
    "pnp-webpack-plugin": "^1.6.4",
    "postcss": "^8.2.1",
    "postcss-loader": "^6.1.1",
    "postcss-normalize": "^10.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.2.1",
    "react-refresh": "^0.10.0",
    "style-loader": "^3.2.1",
    "ts-jest": "^27.0.5",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "typescript": "^4.1.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.10.1",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "3.11.2",
    "webpack-manifest-plugin": "^4.0.2",
    "webpack-merge": "^5.7.0"
  },
  "dependencies": {
    "axios": "^0.21.3",
    "core-js": "^3.8.3",
    "effector": "^22.1.0",
    "effector-react": "^22.0.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,scss,md}": [
      "prettier --write",
      "eslint --cache --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
