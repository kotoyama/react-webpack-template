{
  "name": "react-vite-template",
  "private": true,
  "version": "0.2.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "format": "prettier --write .",
    "lint:code": "eslint \"./src/**/*.{ts,tsx}\"",
    "lint:code:fix": "eslint \"./src/**/*.{ts,tsx}\" --fix",
    "lint:css": "stylelint \"./src/**/*.css\"",
    "lint:css:fix": "stylelint \"./src/**/*.css\" --fix",
    "commit": "cz"
  },
  "dependencies": {
    "effector": "^22.3.0",
    "effector-react": "^22.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.4.3",
    "@types/node": "^18.7.18",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "@vitejs/plugin-react": "^2.1.0",
    "commitizen": "^4.2.5",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-boundaries": "^3.0.0",
    "eslint-plugin-effector": "^0.10.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.8",
    "jsdom": "^20.0.0",
    "nano-staged": "^0.8.0",
    "prettier": "^2.7.1",
    "simple-git-hooks": "^2.8.0",
    "stylelint": "^14.12.0",
    "stylelint-config-css-modules": "^4.1.0",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-recommended": "^9.0.0",
    "stylelint-order": "^5.0.0",
    "typescript": "^4.6.4",
    "vite": "^3.1.0",
    "vite-plugin-checker": "^0.5.1",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-stylelint": "^3.0.7",
    "vite-plugin-svgr": "^2.2.1",
    "vitest": "^0.23.4"
  },
  "resolutions": {
    "@types/testing-library__jest-dom": "github:zoontek/types-testing-library-vitest-dom"
  },
  "simple-git-hooks": {
    "pre-commit": "npx nano-staged"
  },
  "nano-staged": {
    "*.{js,ts}": "prettier --write",
    "*.css": "stylelint"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
