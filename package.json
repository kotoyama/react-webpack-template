{
  "name": "webpack-react-template",
  "version": "0.1.0",
  "private": true,
  "description": "Webpack React Template",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack serve --config config/webpack.dev.js",
    "build": "cross-env NODE_ENV=production webpack --config config/webpack.prod.js",
    "build:live": "serve -s build",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,json,css}",
    "lint:code": "eslint src/**/*.{js,jsx,ts,tsx}",
    "lint:code:fix": "eslint src/**/*.{js,jsx,ts,tsx} --fix",
    "lint:styles": "stylelint \"./**/*.css\"",
    "lint:styles:fix": "stylelint \"./**/*.css\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "commit": "git-cz",
    "pre-commit": "yarn lint-staged",
    "prepare": "husky install"
  },
  "license": "MIT",
  "author": "kotoyama <t.me/kotoyama>",
  "engines": {
    "node": ">= 12.18.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/plugin-transform-modules-commonjs": "^7.15.4",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.0",
    "@svgr/webpack": "^5.5.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.0",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.2.1",
    "@types/core-js": "^2.5.5",
    "@types/jest": "^27.0.1",
    "@types/react": "^17.0.21",
    "@types/react-dom": "^17.0.9",
    "@types/webpack-env": "^1.16.2",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "babel-jest": "^27.2.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-transform-rename-import": "^2.3.0",
    "case-sensitive-paths-webpack-plugin": "^2.4.0",
    "circular-dependency-plugin": "^5.2.2",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "commitizen": "^4.2.4",
    "cross-env": "^7.0.3",
    "css-loader": "^6.2.0",
    "css-minimizer-webpack-plugin": "^3.0.2",
    "cz-conventional-changelog": "3.3.0",
    "dotenv-webpack": "^7.0.3",
    "effector-logger": "^0.12.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-effector": "^0.2.0",
    "eslint-plugin-flowtype": "^6.0.1",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-import-helpers": "^1.1.0",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-react": "^0.0.4",
    "eslint-webpack-plugin": "^3.0.1",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^6.3.3",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^5.3.2",
    "husky": "^7.0.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.2.0",
    "lint-staged": "^11.1.2",
    "mini-css-extract-plugin": "^2.3.0",
    "pnp-webpack-plugin": "^1.7.0",
    "postcss": "^8.3.6",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-loader": "^6.1.1",
    "postcss-normalize": "^10.0.1",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.4.0",
    "react-refresh": "^0.10.0",
    "style-loader": "^3.2.1",
    "stylelint": "^13.13.1",
    "stylelint-config-recess-order": "^2.5.0",
    "stylelint-config-recommended": "^5.0.0",
    "stylelint-order": "^4.1.0",
    "ts-jest": "^27.0.5",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "typescript": "^4.4.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.52.1",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "3.11.2",
    "webpack-manifest-plugin": "^4.0.2",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "axios": "^0.21.4",
    "core-js": "^3.17.3",
    "effector": "^22.1.1",
    "effector-react": "^22.0.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,scss,md}": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
